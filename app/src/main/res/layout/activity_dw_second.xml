<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:fitsSystemWindows="true"
    tools:context="com.example.nj.myapplication.DW_Activity.DW_secondActivity">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/dw_2_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">
        <android.support.v7.widget.Toolbar
            android:id="@+id/dw_2_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@drawable/dw_titlebar"
            app:popupTheme="@style/AppTheme.PopupOverlay" />
    </android.support.design.widget.AppBarLayout>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dw_sec_back"
        android:layout_below="@+id/dw_2_view"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/dw_sec_topText"
        android:layout_below="@+id/dw_2_view"
        android:layout_toRightOf="@+id/dw_sec_back"
        android:layout_toEndOf="@+id/dw_sec_back"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dW_sec_cenImg"
        android:layout_alignWithParentIfMissing="false"
        android:layout_below="@+id/dw_sec_topText"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/dw_sec_midText"
        android:layout_marginTop="58dp"
        android:layout_below="@+id/dW_sec_cenImg"
        android:layout_alignLeft="@+id/dw_sec_botText"
        android:layout_alignStart="@+id/dw_sec_botText"
        android:textSize="35dp"
        android:gravity="center"
        android:layout_marginLeft="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/dw_sec_botText"
        android:layout_below="@+id/dw_sec_midText"
        android:layout_centerHorizontal="true"
        android:textAlignment="gravity"
        android:gravity="center"
        android:textSize="25dp" />


</RelativeLayout>
